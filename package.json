{
  "name": "codealike-code",
  "displayName": "Codealike",
  "description": "Codealike for Visual Studio Code",
  "version": "0.0.25",
  "publisher": "Codealike",
  "license": "MIT",
  "author": {
    "email": "info@codealike.com",
    "name": "Codealike",
    "url": "http://codealike.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Codealike/Codealike-VSCode"
  },
  "bugs": {
    "url": "https://feedback.codealike.com",
    "email": "info@codealike.com"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "*"
  ],
  "keywords": [
    "time-tracking",
    "productivity",
    "management",
    "codealike"
  ],
  "preview": true,
  "homepage": "https://www.codealike.com",
  "icon": "images/AvatarBig.png",
  "galleryBanner": {
    "color": "#c0c0c0"
  },
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codealike.connect",
        "title": "Connect Codealike"
      }
    ]
  },
  "scripts": {
    "lint-removed": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./extension.js --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "package": "webpack --mode production --devtool hidden-source-map --stats-error-details"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@codealike/codealike-core": "0.0.37",
    "esbuild": "^0.16.17"
  }
}
