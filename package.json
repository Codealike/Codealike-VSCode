{
  "name": "codealike-code",
  "displayName": "Codealike",
  "description": "Codealike for Visual Studio Code",
  "version": "0.0.24",
  "publisher": "Codealike",
  "license": "MIT",
  "author": {
    "email": "info@codealike.com",
    "name": "Codealike",
    "url": "http://codealike.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Codealike/Codealike-VSCode"
  },
  "bugs": {
    "url": "https://feedback.codealike.com",
    "email": "info@codealike.com"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "time-tracking",
    "productivity",
    "management",
    "codealike"
  ],
  "preview": true,
  "homepage": "https://www.codealike.com",
  "icon": "images/AvatarBig.png",
  "galleryBanner": {
    "color": "#c0c0c0"
  },
  "engines": {
    "vscode": "^1.15.0"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "codealike.connect",
        "title": "Connect Codealike"
      }
    ]
  },
  "scripts": {
    "postinstall-removed": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./extension.js --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "esbuild": "^0.15.15",
    "eslint": "^8.28.0",
    "mocha": "^10.1.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/vscode": "^1.73.1",
    "@vscode/test-electron": "^2.2.0",
    "ts-loader": "^9.4.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
